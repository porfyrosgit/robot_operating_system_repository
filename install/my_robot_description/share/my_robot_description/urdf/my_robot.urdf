<?xml version="1.0"?>
<robot name="humanoid">

  <link name="base_link">
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <link name="pelvis_link">
    <visual><geometry><cylinder radius="0.18" length="0.22"/></geometry></visual>
    <collision><geometry><cylinder radius="0.18" length="0.22"/></geometry></collision>
    <inertial>
      <mass value="8.0"/>
      <inertia ixx="0.0963" ixy="0" ixz="0" iyy="0.0963" iyz="0" izz="0.1296"/>
    </inertial>
  </link>
  <joint name="base_to_pelvis" type="fixed">
    <parent link="base_link"/><child link="pelvis_link"/>
    <origin xyz="0 0 0.85" rpy="0 0 0"/>
  </joint>

  <link name="torso_link">
    <visual><geometry><cylinder radius="0.13" length="0.60"/></geometry></visual>
    <collision><geometry><cylinder radius="0.13" length="0.60"/></geometry></collision>
    <inertial>
      <mass value="10.0"/>
      <inertia ixx="0.3217" ixy="0" ixz="0" iyy="0.3217" iyz="0" izz="0.0845"/>
    </inertial>
  </link>
  <joint name="pelvis_to_torso" type="fixed">
    <parent link="pelvis_link"/><child link="torso_link"/>
    <origin xyz="0 0 0.41" rpy="0 0 0"/>
  </joint>

  <link name="head_link">
    <visual><geometry><box size="0.22 0.18 0.25"/></geometry></visual>
    <collision><geometry><box size="0.22 0.18 0.25"/></geometry></collision>
    <inertial>
      <mass value="2.0"/>
      <inertia ixx="0.0186" ixy="0" ixz="0" iyy="0.0203" iyz="0" izz="0.0137"/>
    </inertial>
  </link>
  <joint name="torso_to_head" type="fixed">
    <parent link="torso_link"/><child link="head_link"/>
    <origin xyz="0 0 0.425" rpy="0 0 0"/>
  </joint>

  <link name="r_upper_arm_link">
    <visual>
      <origin xyz="0 0 -0.175" rpy="0 0 0"/>
      <geometry><box size="0.11 0.11 0.35"/></geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.175" rpy="0 0 0"/>
      <geometry><box size="0.11 0.11 0.35"/></geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.175" rpy="0 0 0"/>
      <mass value="2.0"/>
      <inertia ixx="0.0223" ixy="0" ixz="0" iyy="0.0223" iyz="0" izz="0.0040"/>
    </inertial>
  </link>
  <joint name="r_shoulder" type="revolute">
    <parent link="torso_link"/><child link="r_upper_arm_link"/>
    <origin xyz="0 -0.185 0.20" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.14" upper="3.14" effort="100" velocity="1.0"/>
    <dynamics damping="0.5" friction="0.1"/>
  </joint>

  <link name="r_forearm_link">
    <visual>
      <origin xyz="0 0 -0.15" rpy="0 0 0"/>
      <geometry><box size="0.09 0.09 0.30"/></geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.15" rpy="0 0 0"/>
      <geometry><box size="0.09 0.09 0.30"/></geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.15" rpy="0 0 0"/>
      <mass value="1.5"/>
      <inertia ixx="0.0120" ixy="0" ixz="0" iyy="0.0120" iyz="0" izz="0.0020"/>
    </inertial>
  </link>
  <joint name="r_elbow" type="revolute">
    <parent link="r_upper_arm_link"/><child link="r_forearm_link"/>
    <origin xyz="0 0 -0.35" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-2.35" upper="0" effort="100" velocity="1.0"/>
    <dynamics damping="0.5" friction="0.1"/>
  </joint>

  <link name="l_upper_arm_link">
    <visual>
      <origin xyz="0 0 -0.175" rpy="0 0 0"/>
      <geometry><box size="0.11 0.11 0.35"/></geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.175" rpy="0 0 0"/>
      <geometry><box size="0.11 0.11 0.35"/></geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.175" rpy="0 0 0"/>
      <mass value="2.0"/>
      <inertia ixx="0.0223" ixy="0" ixz="0" iyy="0.0223" iyz="0" izz="0.0040"/>
    </inertial>
  </link>
  <joint name="l_shoulder" type="revolute">
    <parent link="torso_link"/><child link="l_upper_arm_link"/>
    <origin xyz="0 0.185 0.20" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.14" upper="3.14" effort="100" velocity="1.0"/>
    <dynamics damping="0.5" friction="0.1"/>
  </joint>

  <link name="l_forearm_link">
    <visual>
      <origin xyz="0 0 -0.15" rpy="0 0 0"/>
      <geometry><box size="0.09 0.09 0.30"/></geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.15" rpy="0 0 0"/>
      <geometry><box size="0.09 0.09 0.30"/></geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.15" rpy="0 0 0"/>
      <mass value="1.5"/>
      <inertia ixx="0.0120" ixy="0" ixz="0" iyy="0.0120" iyz="0" izz="0.0020"/>
    </inertial>
  </link>
  <joint name="l_elbow" type="revolute">
    <parent link="l_upper_arm_link"/><child link="l_forearm_link"/>
    <origin xyz="0 0 -0.35" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-2.35" upper="0" effort="100" velocity="1.0"/>
    <dynamics damping="0.5" friction="0.1"/>
  </joint>

  <link name="r_leg_link">
    <visual>
      <origin xyz="0 0 -0.375" rpy="0 0 0"/>
      <geometry><cylinder radius="0.05" length="0.75"/></geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.375" rpy="0 0 0"/>
      <geometry><cylinder radius="0.05" length="0.75"/></geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.375" rpy="0 0 0"/>
      <mass value="3.0"/>
      <inertia ixx="0.1422" ixy="0" ixz="0" iyy="0.1422" iyz="0" izz="0.0038"/>
    </inertial>
  </link>
  <joint name="pelvis_to_r_leg" type="revolute">
    <parent link="pelvis_link"/><child link="r_leg_link"/>
    <origin xyz="0 -0.12 -0.11" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.5" upper="0.5" effort="100" velocity="1.0"/>
    <dynamics damping="0.5" friction="0.1"/>
  </joint>

  <link name="r_wheel_link">
    <visual>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry><cylinder radius="0.10" length="0.06"/></geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry><cylinder radius="0.10" length="0.06"/></geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <inertia ixx="0.0013" ixy="0" ixz="0" iyy="0.0013" iyz="0" izz="0.0050"/>
    </inertial>
  </link>
  <joint name="r_wheel_joint" type="continuous">
    <parent link="r_leg_link"/><child link="r_wheel_link"/>
    <origin xyz="0 0 -0.75" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="0.1" friction="0.5"/>
  </joint>

  <link name="l_leg_link">
    <visual>
      <origin xyz="0 0 -0.375" rpy="0 0 0"/>
      <geometry><cylinder radius="0.05" length="0.75"/></geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.375" rpy="0 0 0"/>
      <geometry><cylinder radius="0.05" length="0.75"/></geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.375" rpy="0 0 0"/>
      <mass value="3.0"/>
      <inertia ixx="0.1422" ixy="0" ixz="0" iyy="0.1422" iyz="0" izz="0.0038"/>
    </inertial>
  </link>
  <joint name="pelvis_to_l_leg" type="revolute">
    <parent link="pelvis_link"/><child link="l_leg_link"/>
    <origin xyz="0 0.12 -0.11" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.5" upper="0.5" effort="100" velocity="1.0"/>
    <dynamics damping="0.5" friction="0.1"/>
  </joint>

  <link name="l_wheel_link">
    <visual>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry><cylinder radius="0.10" length="0.06"/></geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry><cylinder radius="0.10" length="0.06"/></geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <inertia ixx="0.0013" ixy="0" ixz="0" iyy="0.0013" iyz="0" izz="0.0050"/>
    </inertial>
  </link>
  <joint name="l_wheel_joint" type="continuous">
    <parent link="l_leg_link"/><child link="l_wheel_link"/>
    <origin xyz="0 0 -0.75" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="0.1" friction="0.5"/>
  </joint>

  <!-- Differential drive plugin for wheel control -->
  <gazebo>
    <plugin filename="gz-sim-diff-drive-system"
            name="gz::sim::systems::DiffDrive">
      <left_joint>l_wheel_joint</left_joint>
      <right_joint>r_wheel_joint</right_joint>
      <wheel_separation>0.24</wheel_separation>
      <wheel_radius>0.10</wheel_radius>
      <topic>cmd_vel</topic>
      <odom_topic>odom</odom_topic>
      <frame_id>odom</frame_id>
      <child_frame_id>base_link</child_frame_id>
    </plugin>
  </gazebo>

  <!-- Publish joint states back to ROS -->
  <gazebo>
    <plugin filename="gz-sim-joint-state-publisher-system"
            name="gz::sim::systems::JointStatePublisher"/>
  </gazebo>

</robot>